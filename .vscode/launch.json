    "configurations": [
        {
            "name": "Run",
            "type": "lightcode",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug-iphoneos/<App Name>.app",
            "buildCommand": "xcodebuild -configuration Debug -sdk iphoneos -allowProvisioningUpdates",
            "appBundleId": "<Bundle Identifier of the app>"
        },
        {
            "name": "Attach",
            "type": "lightcode",
            "request": "attach",
            "program": "${workspaceFolder}/build/Debug-iphoneos/<App Name>.app",
            "buildCommand": "xcodebuild -configuration Debug -sdk iphoneos -allowProvisioningUpdates",
            "appBundleId": "<Bundle Identifier of the app>"
        }
    ]
}